            <string>///Initialize Console Settings
/* CAN change - Initial console settings */
console_open = false; //console open on screen
console_focused = false; //console being typed into
console_lines = 5; //number of console lines to show + track
console_pos = 'top'; //position of console (top/bottom)
console_font = 'fnt_console'; //the console font
console_text_size = 12; //size of the line text (in point value) - to convert to pixels = point * 96 / 72)
console_text_buffer = 3; //space between lines (in pixels)
console_alpha = 0.75; // the console's alpha (opacity)

/* DO NOT change - system settings and init */
//generate height and position variables
console_line_size = 0;
console_height = 0;
console_xloc = 0;
console_yloc = view_hport[0];
console_outlog_y = 0;
console_outdir_y = 1;
console_offset = 0;
console_dfont = '' + string(console_font) + '_' + string(console_text_size);
scr_console_pref_update();

/* Create the array of console lines */
for (var i = 0; i &lt; console_lines; i++) {
console_array[i] = "" + string(i) + ": Test";
}

show_debug_message("Console obj initialized.");
</string>
