            <string>///Draw console
//TODO: Shift all console calculations to pref update script

if (console_open = true) {
    draw_set_alpha(console_alpha);
    
    /* draw the console background */
    draw_set_colour(c_green);
    draw_rectangle(console_xloc, console_yloc + console_outlog_dir * console_height + (console_outlog_dir * console_offset), console_xloc + view_wport[0]-1,console_yloc,false);
    draw_set_colour(c_lime);
    draw_rectangle(console_xloc, console_yloc + console_outlog_dir * console_height + (console_outlog_dir * console_offset), console_xloc + view_wport[0]-1,console_yloc,true);
    draw_set_colour(c_black);
    
    /* draw the console text */
    draw_set_color(c_white);
    draw_set_font(console_dfont);
    
    for (var i = 0; i &lt; console_lines; i++) {
        draw_text(console_xloc + console_text_buffer, console_outlog_y + console_outlog_dir * (i * (console_height/console_lines)), console_array[i]); //needs to updated to improve position 
    }
    
    draw_set_alpha(1); //reset draw alpha
}
